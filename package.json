{
    "name": "eirys",
    "displayName": "Eirys",
    "author": {
        "name": "Stephen Giandomenico"
    },
    "description": "Personal color theme",
    "version": "0.0.1",
    "type": "module",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Eirys Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/Eirys-dark-color-theme.json"
            },
            {
                "label": "Eirys Light",
                "uiTheme": "vs",
                "path": "./themes/Eirys-light-color-theme.json"
            }
        ]
    },
    "scripts": {
        "generate": "node src/generate_themes.js",
        "test:unit": "NODE_OPTIONS=--experimental-vm-modules npx jest"
    },
    "devDependencies": {
        "@types/jest": "^28.1.4",
        "jest": "^28.1.2"
    }
}