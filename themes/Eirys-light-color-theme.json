{
  "$schema": "vscode://schemas/color-theme",
  "name": "Eirys Light",
  "colors": {
    "editor.background": "#fff",
    "editor.foreground": "#000",
    "editorInlayHint.background": "#0000",
    "editorInlayHint.foreground": "#999"
  },
  "tokenColors": [
    {
      "name": "Source Default",
      "scope": [
        "source",
        "markup.fenced_code meta.embedded"
      ],
      "settings": {
        "foreground": "#666"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#090"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#f00"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.object-literal.key meta string"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "Escape Character",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#06c"
      }
    },
    {
      "name": "Regular Expression",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#c00"
      }
    },
    {
      "name": "Language Constant",
      "scope": [
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "this/self",
      "scope": [
        "variable.language.this",
        "variable.language.special.self",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "foreground": "#990"
      }
    },
    {
      "name": "Color",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "Control Flow Keyword",
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "keyword.generator"
      ],
      "settings": {
        "foreground": "#a3a"
      }
    },
    {
      "name": "Assignment",
      "scope": [
        "keyword.operator.assignment",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#a0a",
        "fontStyle": ""
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#088"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#088"
      }
    },
    {
      "name": "Member",
      "scope": [
        "variable.other.member",
        "variable.other.property",
        "variable.other.object.member",
        "variable.other.object.property",
        "variable.other.constant.property",
        "variable.object.property",
        "meta.object-literal.key",
        "meta.object-literal.key string",
        "meta.member",
        "support.variable.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#740"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#41a"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#093"
      }
    },
    {
      "name": "Type Parameter",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#04e"
      }
    },
    {
      "name": "Type Annotation",
      "scope": [
        "meta.type.annotation support.type",
        "meta.type.annotation entity.name",
        "meta.return.type support.type",
        "meta.return.type entity.name"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#60c"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#17a"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#088"
      }
    },
    {
      "name": "CSS IDs",
      "scope": [
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#740"
      }
    },
    {
      "name": "CSS Pseudo Element",
      "scope": [
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#a80"
      }
    },
    {
      "name": "Markdown - Punctuation",
      "scope": [
        "markup punctuation.definition",
        "text.html.markdown punctuation.definition"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "Markdown - List Punctuation",
      "scope": [
        "markup punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#41a"
      }
    },
    {
      "name": "Markdown - Quote Punctuation",
      "scope": [
        "markup.quote punctuation.definition.quote"
      ],
      "settings": {
        "foreground": "#04e"
      }
    },
    {
      "name": "Markdown - Raw",
      "scope": [
        "text.html.markdown markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "Code Block Language",
      "scope": [
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#093"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup - Strikethrough",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup - Italic-Strikethrough",
      "scope": [
        "markup.italic markup.strikethrough",
        "markup.strikethrough markup.italic"
      ],
      "settings": {
        "fontStyle": "italic strikethrough"
      }
    },
    {
      "name": "Markup - Bold-Strikethrough",
      "scope": [
        "markup.bold markup.strikethrough",
        "markup.strikethrough markup.bold"
      ],
      "settings": {
        "fontStyle": "bold strikethrough"
      }
    },
    {
      "name": "Markup - Bold-Italic-Strikethrough",
      "scope": [
        "markup.bold markup.italic markup.strikethrough",
        "markup.bold markup.strikethrough markup.italic",
        "markup.italic markup.bold markup.strikethrough",
        "markup.italic markup.strikethrough markup.bold",
        "markup.strikethrough markup.bold markup.italic",
        "markup.strikethrough markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "bold italic strikethrough"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#093"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.defaultLibrary": {
      "foreground": "#b60"
    },
    "*.readonly": {
      "italic": true
    },
    "*.declaration": {
      "underline": true
    }
  }
}