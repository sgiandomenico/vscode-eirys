{
  "$schema": "vscode://schemas/color-theme",
  "name": "Eirys Light",
  "colors": {
    "editor.background": "#fff",
    "editor.foreground": "#000"
  },
  "tokenColors": [
    {
      "name": "Source Default",
      "scope": [
        "source"
      ],
      "settings": {
        "foreground": "#666"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#080"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#f00"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.object-literal.key meta string"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "Escape Character",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#06c"
      }
    },
    {
      "name": "Regular Expression",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#c00"
      }
    },
    {
      "name": "Language Constant",
      "scope": [
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "this",
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#990"
      }
    },
    {
      "name": "Control Flow Keyword",
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "keyword.generator"
      ],
      "settings": {
        "foreground": "#a3a"
      }
    },
    {
      "name": "Assignment",
      "scope": [
        "keyword.operator.assignment",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#a0a",
        "fontStyle": ""
      }
    },
    {
      "name": "Declaration",
      "scope": [
        "meta.definition",
        "meta.parameters variable.parameter",
        "meta.object-literal.key"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#088"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#088"
      }
    },
    {
      "name": "Member",
      "scope": [
        "variable.other.member",
        "variable.other.property",
        "variable.other.object.member",
        "variable.other.object.property",
        "variable.object.property",
        "meta.object-literal.key",
        "meta.object-literal.key string",
        "support.variable.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#04a"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "Constant Variable (Global/Default)",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#a80"
      }
    },
    {
      "name": "Local const Variable",
      "scope": [
        "meta.block variable.other.constant"
      ],
      "settings": {
        "foreground": "#740"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#41a"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#093"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#60c"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#17a"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#088"
      }
    },
    {
      "name": "CSS IDs",
      "scope": [
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#740"
      }
    },
    {
      "name": "CSS Pseudo Element",
      "scope": [
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#a80"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "Markdown - Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw"
      ],
      "settings": {
        "foreground": "#c30"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": {
      "italic": true
    },
    "variable.readonly": {
      "foreground": "#a80"
    },
    "variable.local": {
      "italic": false
    },
    "variable.local.readonly": {
      "foreground": "#740"
    },
    "variable.defaultLibrary": {
      "foreground": "#a80"
    }
  }
}